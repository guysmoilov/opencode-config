{
  "$schema": "https://opencode.ai/config.json",
  "mcp": {
    "playwright": {
      "type": "local",
      "command": [
        "npx",
        "@playwright/mcp@latest"
      ],
      "enabled": true
    }
  },
  "permission": {
    "edit": "allow",
    "bash": {
        "*": "ask",
        "rg *": "allow",
        "grep *": "allow",
        "ls *": "allow",
        "cd *": "allow",
        "git status *": "allow",
        "git diff *": "allow",
        "git log *": "allow",
        "git branch *": "allow",
        "git show *": "allow",
        "git config *": "allow",
        "git rev-parse *": "allow",
        "git ls-files *": "allow",
        "git cat-file *": "allow",
        "git for-each-ref *": "allow",
        "git rev-list *": "allow",
        "git describe *": "allow",
        "git symbolic-ref *": "allow",
        "git name-rev *": "allow",
        "git merge-base *": "allow",
        "git diff-tree *": "allow",
        "git ls-tree *": "allow",
        "git ls-remote *": "allow",
        "go test *": "allow",
        "npx tsc *": "allow",
        "tsc *": "allow",
        "pwd": "allow",
        "wc *": "allow",
        "stat *": "allow",
        "file *": "allow",
        "npm test *": "allow",
        "pytest *": "allow",
        "cargo test *": "allow",
        "which *": "allow",
        "date *": "allow",
        "uname *": "allow",
        "head *": "allow",
        "tail *": "allow",
        "find *": "allow",
        "bash -n *": "allow",
        "zsh -n *": "allow",
        "test *": "allow",
        "echo *": "allow"
    },
    "skill": "allow",
    "webfetch": "ask",
    "doom_loop": "ask",
    "external_directory": "ask"
  },
